{
    "private": true,
    "version": "0.0.2",
    "name": "@pleo-io/spa-edge-lambdas",
    "dependencies": {
        "aws-sdk": "2.1370.0"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.115",
        "@types/jest": "29.5.1",
        "@types/node": "18.16.3",
        "@vercel/ncc": "0.36.1",
        "@swc/jest": "0.2.26",
        "jest": "29.5.0",
        "prettier": "2.8.8",
        "typescript": "5.0.4"
